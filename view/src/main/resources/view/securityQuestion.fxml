            <Button layoutX="80.0" layoutY="74.0" mnemonicParsing="false" onAction="#yesButtonPressed" prefHeight="29.0" prefWidth="93.0" text="YES" />
            <Button layoutX="232.0" layoutY="74.0" mnemonicParsing="false" onAction="#noButtonPressed" prefHeight="29.0" prefWidth="93.0" text="NO" />
         </children>
    </AnchorPane>
  </children>
